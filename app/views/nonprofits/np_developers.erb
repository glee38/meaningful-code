<h1>Your Developers</h1><br>
<%if @np.developers.count == 0%>
  No developers to show.
<%else%>
  <% @np.developers.uniq.each_with_index do |d,i|%>

    <h4><p class="text-warning"><%="#{i+1}. "%><a href="/developers/<%=d.slug%>"><%= d.name %></a> // <a href="<%=d.github%>"><%= d.github %></a></h4>
  
  <ul>
    <%d.projects.each do |p|%>
      <% if p.nonprofit == current_np %>
        <li><%=p.name%></li>
      <%end%>
    <%end%><br>
  </ul>

  <%end%>
<%end%>