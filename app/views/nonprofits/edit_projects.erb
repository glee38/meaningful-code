<% unless locals.empty? %>
  <%= message %>
<% end %>

<h1>Manage Projects</h1><br>
<%if current_np.projects.count == 0%>
  No current projects.
<%else%>
  <%current_np.projects.each do |p|%>

  <form method="post" action="/nonprofits/<%=current_np.slug%>/project/edit">

    <h5><p class="text-muted"><input type="radio" name="project_id" value="<%=p.id%>"> &nbsp;<a href="/nonprofits/<%=p.nonprofit.slug%>/projects/<%=p.slug%>"><%=p.name%></a></p></h5>
  <%end%><br>
    <input type="submit" class="link" value="Edit Project"><br>
  </form><br>
<%end%><br>

  
