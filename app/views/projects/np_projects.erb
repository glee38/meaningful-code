<h1><%=@np.name%></h1>
<h3><p class="text-primary"><%=@np.tagline%></h3><br>
<h3>Projects</h3>
<%if @np.projects.count == 0%>
  There are no projects to display.
<%else%>
  <% @np.projects.each do |p| %>
  <li><a href="/nonprofits/<%=@np.slug%>/projects/<%=p.slug%>"><h4><p class="text-muted"><%= p.name %></h4></a></li>
  <%end%>
<%end%>