<% unless locals.empty? %>
  <%= message %>
<% end %>

<h1>Manage Projects</h1><br>
<%if current_dev.projects.count == 0%>
  No current projects.

<%else%>
  <%current_dev.projects.each do |p|%>

  <form method="post" action="/developers/<%=current_dev.slug%>/projects/edit">

    <h5><p class="text-muted"><input type="checkbox" name="dev[project_ids][]" value="<%=p.id%>"> &nbsp;<a href="/nonprofits/<%=p.nonprofit.slug%>/projects/<%=p.slug%>"><%=p.name%></a> // <a href="/nonprofits/<%=p.nonprofit.slug%>"><%=p.nonprofit.name%></a></p></h5>
  <%end%><br>

    <input id="hidden" type="hidden" name="_method" value="PATCH">
    <input type="submit" value="Remove Project(s)"></p>

  </form><br>

<%end%><br>
  

